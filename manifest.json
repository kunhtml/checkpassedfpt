{
  "name": "Tự Động Check Passed FPT",
  "version": "1.0.0",
  "description": "Tự động kiểm tra điểm và F5 trang FPT đến khi Passed",
  "manifest_version": 3,
  "permissions": ["activeTab", "storage", "notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://fap.fpt.edu.vn/Grade/StudentGrade.aspx*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Tự Động Check Passed FPT",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
